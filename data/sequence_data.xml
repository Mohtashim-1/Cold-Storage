<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Intake Sequence -->
        <record id="seq_cs_storage_intake" model="ir.sequence">
            <field name="name">Cold Storage Intake</field>
            <field name="code">cs.storage.intake</field>
            <field name="prefix">INT/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Release Sequence -->
        <record id="seq_cs_stock_release" model="ir.sequence">
            <field name="name">Cold Storage Release</field>
            <field name="code">cs.stock.release</field>
            <field name="prefix">REL/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Contract Sequence -->
        <record id="seq_cs_storage_contract" model="ir.sequence">
            <field name="name">Cold Storage Contract</field>
            <field name="code">cs.storage.contract</field>
            <field name="prefix">CON/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Gate Entry Sequence -->
        <record id="seq_cs_gate_entry" model="ir.sequence">
            <field name="name">Gate Entry (In)</field>
            <field name="code">cs.gate.entry</field>
            <field name="prefix">GIN/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Gate Exit Sequence -->
        <record id="seq_cs_gate_exit" model="ir.sequence">
            <field name="name">Gate Entry (Out)</field>
            <field name="code">cs.gate.exit</field>
            <field name="prefix">GOUT/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
        
    </data>
</odoo>
